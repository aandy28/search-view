{
  "presets": ["env", "react"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ]
      ],
      "presets": [["babel-preset-minify",
        {
          "mangle": false,
          "evaluate": false
        }]]
    }
  }
}
